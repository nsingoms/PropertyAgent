@page "/"
@inject IProperty PropertyService


<div class="col-md-6">
            <h1>Properties</h1>
        </div>
  <div class="row">
    @foreach(var prop in properties)
    {
          <PropertyComponent property="prop" ></PropertyComponent>
    }
  
   </div>
   
@code{

    IEnumerable<PropertyDto> properties = new List<PropertyDto>();
   

    protected override async Task OnInitializedAsync()
    {
        properties = await PropertyService.GetPropertiesAsync();
    }
   

   }